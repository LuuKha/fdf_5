<tr>
   <td><%= order.id %></td>
   <td><%= order.created_at %></td>
   <td><%= order.cost %></td>
   <td><%= order.payment_method %></td>
   <% if @type_paid == "paid" %>
     <td><%= order.paid_at %></td>
   <% end %>
   <td>
     <%= link_to t("order.view"), order, class: "btn btn-primary" %>
   </td>
 </tr>
