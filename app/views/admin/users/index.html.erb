<% provide :title, t("application.all_users") %>
<h1><%= t "application.users" %></h1>
<div class="list_users">
  <div class="paginate">
    <%= will_paginate %>
  </div>

  <ul class="users">
    <% @users.each do |user| %>
      <li>
        <%= render user %>
      </li>
    <% end %>
  </ul>

  <div class="paginate">
    <%= will_paginate %>
  </div>
</div>
